<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title></title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdn.jsdelivr.net/npm/vue@2.6.12/dist/vue.js"></script>

    {% for id, block in csrBlocks %}
    <!-- id -->
    <style>
    {{ block.css | safe }}
    </style>
    {% endfor %} 
</head>
<body>

    <div>
        <h1>{{title}}</h1>
        <p>{{message}}</p>
        {{ renderComponent('mybutton', { title: title }) | await | safe }}
    </div>


    {% for id, block in csrBlocks %}
    <!-- id -->
    <script type="module">
    {{ block.js | safe }}
    </script>
    {% endfor %} 

</body>
</html>


